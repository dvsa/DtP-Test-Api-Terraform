{
  "name": "fake-DtP-api",
  "version": "1.0.0",
  "description": "",
  "main": "src/index.js",
  "directories": {
    "test": "test"
  },
  "dependencies": {},
  "devDependencies": {
    "chai": "^4.1.2",
    "eslint": "^4.8.0",
    "eslint-config-airbnb": "^16.0.0",
    "eslint-plugin-import": "^2.7.0",
    "eslint-plugin-jsx-a11y": "^6.0.2",
    "eslint-plugin-react": "^7.4.0",
    "husky": "^0.14.3",
    "mocha": "^4.0.1",
    "nodemon": "^1.12.1",
    "nyc": "^11.3.0",
    "retire": "^1.4.0"
  },
  "nyc": {
    "check-coverage": true,
    "per-file": true,
    "lines": 90,
    "statements": 90,
    "functions": 90,
    "branches": 90,
    "include": [
      "src/**/*.js"
    ],
    "extension": [
      ".js"
    ],
    "cache": true,
    "all": true
  },
  "scripts": {
    "precommit": "npm test",
    "test": "nyc mocha \"test/**/*.test.js\" --exit",
    "test:watch": "nodemon --exec \"npm run test\"",
    "retire": "retire -n -p",
    "lint": "eslint .",
    "lint:watch": "nodemon --exec \"npm run lint\"",
    "prod": "npm prune --production && npm install --production",
    "report": "nyc report --reporter=html",
    "build": "npm install && npm run lint && npm run retire && npm test && mkdir -p ./dist && npm run prod && zip -r ./dist/fakeDtPapi.zip node_modules/ src/"
  },
  "author": "Marcin Duszynski",
  "license": "MIT"
}
